<?xml version="1.0" ?>
<organization>
<folderstructure>
<folder name="main"></folder>
<folder name="stylesheets">
<folder name="fonts"></folder>
<folder name="images">
<folder name="hd">
</folder>
</folder>
</folder>
<folder name="scripts">
<folder name="libs">
</folder>
</folder>
<folder name="txts"></folder>
</folderstructure>
<file-orientation>
<for folder="stylesheets">
<sor how="FileTypeSpecification" spec=".css"></sor>
</for>
<for folder="scripts/libs">
<sor how="FullNameSpecification" spec="simplelib-4.9.0.min"></sor>
<sor how="FileTypeSpecification" spec=".js"></sor>
</for>
<for folder="scripts">
<sor how="FileTypeSpecification" spec=".js"></sor>
</for>
<for folder="stylesheets/images">
<sor how="FileTypeSpecification" spec=".png"></sor>
</for>
<for folder="stylesheets/images">
<sor how="FileTypeSpecification" spec=".jgp"></sor>
</for>
<for folder="stylesheets/fonts">
<sor how="FileTypeSpecification"  spec=".otf"></sor>
</for>
<for>
<sor folder="stylesheets/fonts" how="FileTypeSpecification" spec=".ttf"></sor>
</for>
<for folder="txts">
<sor how="PrefixSpecification" spec="txxt-"></sor>
<sor how="FileTypeSpecification" spec=".txt"></sor>
</for>
</file-orientation>
</organization>